{
  "name": "the-orcs-express",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "npm-run-all --parallel start:dev typesafe-i18n-generator",
    "start:dev": "cross-env NODE_ENV=dev tsnd  --unhandled-rejections=strict  --trace-deprecation  --trace-warnings  --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:unit && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "typesafe-i18n-generator": "typesafe-i18n"
  },
  "dependencies": {
    "@tsed/ajv": "^6.64.1",
    "@tsed/common": "^6.64.1",
    "@tsed/core": "^6.64.1",
    "@tsed/di": "^6.64.1",
    "@tsed/exceptions": "^6.64.1",
    "@tsed/json-mapper": "^6.64.1",
    "@tsed/mongoose": "^6.64.1",
    "@tsed/passport": "^6.64.1",
    "@tsed/platform-express": "^6.64.1",
    "@tsed/schema": "^6.64.1",
    "ajv": "^8.6.2",
    "ajv-i18n": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongodb-session": "^3.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-flash-message": "^2.1.0",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "method-override": "^3.0.0",
    "mongoose": "^6.0.3",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "nodemailer-mailgun-transport": "^2.1.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "typesafe-i18n": "^2.37.4"
  },
  "devDependencies": {
    "@tsed/cli-plugin-mocha": "3.11.2",
    "@tsed/cli-plugin-mongoose": "3.11.2",
    "@tsed/testing-mongoose": "^6.64.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/compression": "^1.7.1",
    "@types/connect-mongodb-session": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/method-override": "^0.0.32",
    "@types/mocha": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.8",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-local": "^1.0.34",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  }
}
